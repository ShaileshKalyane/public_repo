pipeline {
    agent any
    
    stages {
        stage('Checkout') {
            steps {
                checkout scmGit(
                branches: [[name: '*/main']], 
                extensions: [], 
                userRemoteConfigs: [[url: 'https://github.com/ShaileshKalyane/public_repo.git']])
            }
        }
        stage('Test') {
            steps {
                sh """
					         #!/bin/bash
					          echo This is Integration testing
					          sleep 5
				           	ls -lrt
			          """
            }
        }
    }
}
